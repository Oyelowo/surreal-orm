IF age >= 18 <= 120 
	{ SELECT * FROM user:oyelowo WHERE (city IS 'Prince Edward Island') AND (city IS 'NewFoundland') OR (city ~ 'Toronto') ORDER BY age NUMERIC LIMIT 153 START AT 10 PARALLEL; }
ELSE IF name ~ 'Oyelowo Oyedayo' 
	{ SELECT * FROM user:oyedayo WHERE country IS 'INDONESIA' ORDER BY age NUMERIC LIMIT 20 START AT 5; }
ELSE IF (country IS 'Canada') OR (country IS 'Norway') 
	{ SELECT * FROM fruit; }
ELSE
	{ SELECT * FROM book; };
