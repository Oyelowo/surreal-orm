response=$(
  timeout -s 3 $1 \\
  redis-cli \\
    -h localhost \\
    -p $REDIS_PORT \\
    ping
)
if [ "$?" -eq "124" ]; then
  echo "Timed out"
  exit 1
fi
responseFirstWord=$(echo $response | head -n1 | awk '{print $1;}')
