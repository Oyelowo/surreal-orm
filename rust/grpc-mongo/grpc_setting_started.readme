<p><strong>Client</strong><br>
</p>

<div class="highlight js-code-highlight">
<pre class="highlight rust"><code>    <span class="k">use</span> <span class="nn">futures</span><span class="p">::</span><span class="nn">stream</span><span class="p">::</span><span class="n">iter</span><span class="p">;</span>
    <span class="k">use</span> <span class="nn">hello</span><span class="p">::</span><span class="nn">say_client</span><span class="p">::</span><span class="n">SayClient</span><span class="p">;</span>
    <span class="k">use</span> <span class="nn">hello</span><span class="p">::</span><span class="n">SayRequest</span><span class="p">;</span>
    <span class="k">mod</span> <span class="n">hello</span><span class="p">;</span>
    <span class="nd">#[tokio::main]</span>
    <span class="k">async</span> <span class="k">fn</span> <span class="nf">main</span><span class="p">()</span> <span class="k">-&gt;</span> <span class="n">Result</span><span class="o">&lt;</span><span class="p">(),</span> <span class="nb">Box</span><span class="o">&lt;</span><span class="n">dyn</span> <span class="nn">std</span><span class="p">::</span><span class="nn">error</span><span class="p">::</span><span class="n">Error</span><span class="o">&gt;&gt;</span> <span class="p">{</span>
        <span class="k">let</span> <span class="n">channel</span> <span class="o">=</span> <span class="nn">tonic</span><span class="p">::</span><span class="nn">transport</span><span class="p">::</span><span class="nn">Channel</span><span class="p">::</span><span class="nf">from_static</span><span class="p">(</span><span class="s">"http://[::1]:50051"</span><span class="p">)</span>
            <span class="nf">.connect</span><span class="p">()</span>
            <span class="k">.await</span><span class="o">?</span><span class="p">;</span>
        <span class="k">let</span> <span class="k">mut</span> <span class="n">client</span> <span class="o">=</span> <span class="nn">SayClient</span><span class="p">::</span><span class="nf">new</span><span class="p">(</span><span class="n">channel</span><span class="p">);</span>
    <span class="c">// creating a client</span>
        <span class="k">let</span> <span class="n">request</span> <span class="o">=</span> <span class="nn">tonic</span><span class="p">::</span><span class="nn">Request</span><span class="p">::</span><span class="nf">new</span><span class="p">(</span><span class="nf">iter</span><span class="p">(</span><span class="nd">vec!</span><span class="p">[</span>
            <span class="n">SayRequest</span> <span class="p">{</span>
                <span class="n">name</span><span class="p">:</span> <span class="nn">String</span><span class="p">::</span><span class="nf">from</span><span class="p">(</span><span class="s">"anshul"</span><span class="p">),</span>
            <span class="p">},</span>
            <span class="n">SayRequest</span> <span class="p">{</span>
                <span class="n">name</span><span class="p">:</span> <span class="nn">String</span><span class="p">::</span><span class="nf">from</span><span class="p">(</span><span class="s">"rahul"</span><span class="p">),</span>
            <span class="p">},</span>
            <span class="n">SayRequest</span> <span class="p">{</span>
                <span class="n">name</span><span class="p">:</span> <span class="nn">String</span><span class="p">::</span><span class="nf">from</span><span class="p">(</span><span class="s">"vijay"</span><span class="p">),</span>
            <span class="p">},</span>
        <span class="p">]));</span>
    <span class="c">// calling rpc</span>
        <span class="k">let</span> <span class="k">mut</span> <span class="n">response</span> <span class="o">=</span> <span class="n">client</span><span class="nf">.bidirectional</span><span class="p">(</span><span class="n">request</span><span class="p">)</span><span class="k">.await</span><span class="o">?</span><span class="nf">.into_inner</span><span class="p">();</span>
    <span class="c">// listening on the response stream</span>
        <span class="k">while</span> <span class="k">let</span> <span class="nf">Some</span><span class="p">(</span><span class="n">res</span><span class="p">)</span> <span class="o">=</span> <span class="n">response</span><span class="nf">.message</span><span class="p">()</span><span class="k">.await</span><span class="o">?</span> <span class="p">{</span>
            <span class="nd">println!</span><span class="p">(</span><span class="s">"NOTE = {:?}"</span><span class="p">,</span> <span class="n">res</span><span class="p">);</span>
        <span class="p">}</span>
        <span class="nf">Ok</span><span class="p">(())</span>
    <span class="p">}</span>
</code></pre>
