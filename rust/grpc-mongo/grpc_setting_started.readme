       <span class="nn">tonic_build</span><span class="p">::</span><span class="nf">compile_protos</span><span class="p">(</span><span class="s">"proto/say.proto"</span><span class="p">)</span><span class="o">?</span><span class="p">;</span>
       <span class="nf">Ok</span><span class="p">(())</span>
    <span class="p">}</span>
