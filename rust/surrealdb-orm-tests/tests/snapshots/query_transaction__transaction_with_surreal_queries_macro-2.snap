BEGIN TRANSACTION;

LET $balance = (CREATE balance CONTENT { balance: 300.0, id: balance:balance });

CREATE account CONTENT { balance: 135605.16, id: account:one };

CREATE account CONTENT { balance: 91031.31, id: account:two };

UPDATE account:one SET balance += $balance.balance;

UPDATE account:two SET balance -= 300.0;

COMMIT TRANSACTION;
	
